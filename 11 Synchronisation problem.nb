  3.7494865969755573`*^9, 3.749486682937333*^9}, {3.7494867356777287`*^9, 
  3.749486759673727*^9}},ExpressionUUID->"88ca8c1d-1f16-43f9-960d-\
b6a6c9d88e6b"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SynchProblemFailure", "[", 
    RowBox[{"rnum_Integer", ",", 
     RowBox[{"k_Integer:", " ", "2"}], ",", 
     RowBox[{"r_:", " ", "1"}]}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ic", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{
          RowBox[{"k", "-", "1"}], ",", 
          RowBox[{"\[LeftFloor]", 
           RowBox[{
            RowBox[{"2", "r"}], "+", "1"}], "\[RightFloor]"}]}], "]"}]}], ",", 
       RowBox[{"neighbourhoods", "=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"neighsize", "=", 
        RowBox[{"\[LeftFloor]", 
         RowBox[{
          RowBox[{"2", "r"}], "+", "1"}], "\[RightFloor]"}]}], ",", 
       "icprevious", ",", "loopedNeighbourhoods", ",", "loopedIC"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"icprevious", "=", "ic"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"\[Not]", 
         RowBox[{"MemberQ", "[", 
          RowBox[{"neighbourhoods", ",", 
           RowBox[{"Take", "[", 
            RowBox[{"icprevious", ",", 
             RowBox[{"-", "neighsize"}]}], "]"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"neighbourhoods", "=", 
          RowBox[{"Join", "[", 
           RowBox[{"neighbourhoods", ",", 
            RowBox[{"{", 
             RowBox[{"Take", "[", 
              RowBox[{"ic", ",", 
               RowBox[{"-", "neighsize"}]}], "]"}], "}"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"ic", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"ic", ",", 
            RowBox[{
             RowBox[{"Cases", "[", 
              RowBox[{
               RowBox[{"RuleTable", "[", 
                RowBox[{"rnum", ",", "k", ",", "r"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"Take", "[", 
                  RowBox[{"ic", ",", 
                   RowBox[{"-", "neighsize"}]}], "]"}], ",", "_"}], "}"}]}], 
              "]"}], "\[LeftDoubleBracket]", 
             RowBox[{"1", ",", 
              RowBox[{"-", "1"}]}], "\[RightDoubleBracket]"}]}], "]"}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"icprevious", "=", "ic"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"loopedNeighbourhoods", "=", 
       RowBox[{"Take", "[", 
        RowBox[{"neighbourhoods", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{"neighbourhoods", ",", 
              RowBox[{"Take", "[", 
               RowBox[{"ic", ",", 
