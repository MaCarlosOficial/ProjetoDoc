 RowBox[{
  RowBox[{"completarLista", "[", 
   RowBox[{"l_List", ",", "n_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"soma", ",", "faltam", ",", "completar"}], "}"}], ",", 
